{
  "dxt_version": "0.1",
  "name": "acme-orchestrator-proxy",
  "display_name": "Acme Orchestrator",
  "version": "1.0.0",
  "description": "Connects Claude Desktop to the internal Acme Robotics knowledge base via HTTP/SSE.",
  "author": {
    "name": "Acme Corp"
  },
  "server": {
    "type": "python",
    "entry_point": "server/proxy.py",
    "mcp_config": {
      "command": "python",
      "args": ["${__dirname}/server/proxy.py"],
      "env": {
        "MCP2_URL": "http://127.0.0.1:8002",
        "PYTHONPATH": "${__dirname}/lib"
      }
    }
  }
}